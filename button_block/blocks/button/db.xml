<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.concrete5.org/doctrine-xml/0.5" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.concrete5.org/doctrine-xml/0.5  http://concrete5.github.io/doctrine-xml/doctrine-xml-0.5.xsd">
	<table name="btButtons">
		<field name="bID" type="integer" size="10">
			<unsigned/>
			<autoincrement/>
			<key/>
		</field>
		<field name="size" type="string">
			<default value="NULL" />
		</field>
		<field name="style" type="string">
			<default value="" />
		</field>
		<field name="align" type="string">
			<default value="" />
		</field>
		<field name="intern" type="boolean">
			<default value="1" />
		</field>
		<field name="internalLinkCID" type="integer" size="10">
			<unsigned/>
		</field>
		<field name="url" type="string">
			<default value="" />
		</field>
		<field name="text" type="string">
			<default value="" />
		</field>
		<field name="class" type="string">
			<default value="" />
		</field>
		<field name="id" type="string">
			<default value="" />
		</field>
		<field name="target" type="string">
			<default value="" />
		</field>
	</table>
</schema>
